<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.web>
    <compilation debug="true" />
  </system.web>
  <system.serviceModel>
    <services>
      <service name="lr7.Feed1" behaviorConfiguration="lr7.serviceBehaviour">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:40001/SyndicationService/" />
          </baseAddresses>
        </host>
        <endpoint contract="lr7.IFeed1" address="Feed1" binding="webHttpBinding" behaviorConfiguration="lr7.Feed1Behavior"/>
        <endpoint contract="IMetadataExchange" binding="mexHttpBinding" address="mex"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="lr7.serviceBehaviour">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="lr7.Feed1Behavior">
          <webHttp/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
